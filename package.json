{
  "name": "dibk-wizard-framework",
  "version": "1.0.0",
  "description": "Wizard rendering framework",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "yarn run lint:js",
    "lint:js": "eslint . --ext .jsx,.js --ignore-pattern dist",
    "build": "NODE_ENV=production webpack -p",
    "dev": "webpack --watch",
    "start": "yarn run dev"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netliferesearch/dibk-wizard-framework.git"
  },
  "keywords": [
    "wizard",
    "framework",
    "react",
    "dibk"
  ],
  "author": "Netlife Research",
  "private": true,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netliferesearch/dibk-wizard-framework/issues"
  },
  "homepage": "https://github.com/netliferesearch/dibk-wizard-framework#readme",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-autobind": "^1.0.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "redux": "^3.7.1",
    "styled-components": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-inline-import": "^2.0.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "pre-commit": "^1.2.2",
    "webpack": "^3.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-confusing-arrow": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/no-children-prop": 0
    }
  }
}
