{
  "name": "losen",
  "version": "0.18.0",
  "description": "Losen - a flexible wizard rendering framework",
  "main": "dist/bundle.js",
  "bin": {
    "losen": "./bin/cli.js",
    "losen-validate": "./bin/validate.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --cache --cache-location=.cache/eslint --ext .jsx,.js",
    "build": "NODE_ENV=production webpack",
    "dev": "webpack --watch",
    "start": "npm run dev",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netliferesearch/losen.git"
  },
  "keywords": [
    "wizard",
    "framework",
    "react",
    "dibk"
  ],
  "contributors": [
    {
      "name": "Kristoffer Brabrand",
      "email": "kristoffer@brabrand.no"
    },
    {
      "name": "Anders Olsen Sandvik",
      "email": "anders.sandvik@netlife.com"
    },
    {
      "name": "Elise Jakobsen",
      "email": "hei@elisejakob.no"
    }
  ],
  "license": "SEE LICENSE IN LICENCE.md",
  "bugs": {
    "url": "https://github.com/netliferesearch/losen/issues"
  },
  "homepage": "https://github.com/netliferesearch/losen#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.setwith": "^4.3.2",
    "lodash.template": "^4.4.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-autobind": "^1.0.6",
    "react-modal": "^3.0.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "styled-components": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "pre-commit": "^1.2.2",
    "react-dom": "^15.6.2",
    "webpack": "^3.6.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-return-assign": 0,
      "no-confusing-arrow": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/no-children-prop": 0
    },
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
