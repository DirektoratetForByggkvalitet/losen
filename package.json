{
  "name": "losen",
  "version": "5.1.0",
  "description": "Losen - a flexible wizard rendering framework",
  "main": "dist/bundle.js",
  "bin": {
    "losen": "./bin/cli.js",
    "losen-validate": "./bin/validate.js"
  },
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint . --ext .jsx,.js",
    "build": "NODE_ENV=production webpack",
    "dev": "webpack --watch",
    "start": "npm run dev",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "prepublishOnly": "npm run build && npm run lint && npm run test",
    "deploy": "npm run build-storybook && firebase deploy",
    "release": "np"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netliferesearch/losen.git"
  },
  "keywords": [
    "wizard",
    "framework",
    "react",
    "dibk"
  ],
  "contributors": [
    {
      "name": "Kristoffer Brabrand",
      "email": "kristoffer@brabrand.no"
    },
    {
      "name": "Anders Olsen Sandvik",
      "email": "anders.sandvik@netlife.com"
    },
    {
      "name": "Elise Jakobsen",
      "email": "hei@elisejakob.no"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netliferesearch/losen/issues"
  },
  "homepage": "https://github.com/netliferesearch/losen#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "details-polyfill": "^1.1.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^3.7.2",
    "styled-components": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "np": "^3.0.4",
    "pre-commit": "^1.2.2",
    "react-dom": "^15.6.2",
    "react-json-pretty": "^1.7.6",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.1.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-debugger": 0,
      "no-return-assign": 0,
      "no-confusing-arrow": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/no-children-prop": 0
    },
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary"
    ]
  }
}
