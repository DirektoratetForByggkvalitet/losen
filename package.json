{
  "name": "dibk-wizard-framework",
  "version": "1.0.0",
  "description": "Wizard rendering framework",
  "main": "dist/bundle.js",
  "bin": {
    "dibk-wizard-framework": "./bin/cli.js",
    "dibk-wizard-framework-validate": "./bin/validate.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --cache --cache-location=.cache/eslint --ext .jsx,.js --ignore-path .gitignore",
    "build": "NODE_ENV=production webpack -p",
    "dev": "webpack --watch",
    "start": "npm run dev",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netliferesearch/dibk-wizard-framework.git"
  },
  "keywords": [
    "wizard",
    "framework",
    "react",
    "dibk"
  ],
  "author": "Netlife Research",
  "private": true,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netliferesearch/dibk-wizard-framework/issues"
  },
  "homepage": "https://github.com/netliferesearch/dibk-wizard-framework#readme",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "commander": "^2.11.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.setwith": "^4.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-autobind": "^1.0.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "redux": "^3.7.1",
    "styled-components": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.8",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-inline-import": "^2.0.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^20.0.4",
    "pre-commit": "^1.2.2",
    "react-dom": "^15.6.1",
    "webpack": "^3.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-return-assign": 0,
      "no-confusing-arrow": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/no-children-prop": 0,
      "jsx-a11y/accessible-emoji": 0
    },
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
